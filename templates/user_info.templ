package templates

templ UserInfo(name *string, email *string, provider *string, userID *string) {
    <div class="user-info">
        <h2 class="text-lg font-semibold">Your Profile</h2>
        <p>Name: { *name }</p>
        <p>Email: { *email }</p>
        <p>Provider: { *provider }</p>
        <p>User ID: { *userID }</p>
    </div>
}


